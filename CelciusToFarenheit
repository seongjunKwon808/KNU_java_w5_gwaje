public class CelciusToFarenheit {
    public static void main(String[] args) {

        //이 함수는 섭씨 온도를 화씨 온도로 바꿔주는 함수입니다!
        for (int i = -40; i <= 100;i = i + 5){
            System.out.printf("C = %d --> ",i);
            covertAndPrint(i);
            System.out.print("\n");
        }



    }

    public static void covertAndPrint(double c) {
        double f = c * 9.0 / 5.0 + 32.0;
        System.out.print(f);
    }
}
